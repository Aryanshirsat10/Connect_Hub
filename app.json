{
  "expo": {
    "name": "ConnectHub",
    "slug": "ConnectHub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo8.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/ConnectHub.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker"
    ],
    "permissions": [
      {
        "photo": {
          "description": "The app accesses your photos to let you share them with your friends."
        }
      }
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
