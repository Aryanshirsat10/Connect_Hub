{
  "expo": {
    "name": "ConnectHub",
    "slug": "connecthub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo8.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/ConnectHub.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aryan10.connecthub"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon1.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.aryan10.connecthub"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker"
    ],
    "permissions": [
      {
        "photo": {
          "description": "The app accesses your photos to let you share them with your friends."
        }
      }
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "395b7259-abe0-436b-bec6-8021dca31134"
      }
    },
    "owner": "aryan10"
  }
}
